{
  "name": "gemini-api-server",
  "version": "1.0.0",
  "description": "High-performance Gemini Business API Gateway",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "node src/server.js",
    "setup-db": "node scripts/setup-database.js",
    "reset-db": "node scripts/reset-database.js",
    "test": "node tests/test-api.js",
    "build:ui": "cd admin-ui && npm install && npm run build",
    "docker:build": "docker build -f docker/Dockerfile -t gemini-gateway .",
    "docker:up": "docker-compose -f docker/docker-compose.yml up -d",
    "docker:down": "docker-compose -f docker/docker-compose.yml down",
    "docker:multi": "docker-compose -f docker/docker-compose.yml --profile multi-instance up -d",
    "docker:nginx": "docker-compose -f docker/docker-compose.yml --profile with-nginx up -d"
  },
  "keywords": [
    "gemini",
    "api",
    "gateway",
    "google"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^10.0.2",
    "@fastify/rate-limit": "^10.2.0",
    "@fastify/static": "^8.0.4",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.7",
    "fastify": "^5.2.1",
    "fastify-plugin": "^5.0.1",
    "mysql2": "^3.12.0",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0"
  }
}
